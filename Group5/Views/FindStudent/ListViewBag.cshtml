
@{
    Layout = null;
    List<Group5.Models.College> obj = ViewBag.list;
    List<Group5.Models.College> filter = ViewBag.filter;
}

<!DOCTYPE html>

<html>          
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <title>ListViewBag</title>
</head>
<body>
    <div>
        <h1>Danh sach sinh vien</h1>
        @foreach (var i in obj)
        {
            <ul class="list-group list-group-horizontal">
                <li class="list-group-item px-2" style="width:100px">@i.mssv</li>
                <li class="list-group-item px-2" style="width:200px">@i.Name</li>
                <li class="list-group-item px-2" style="width:100px">@i.Class</li>
            </ul>
        }
        <div style="height: 30px"></div>
        <h1>Danh sach sinh vien lop 63cntt4</h1>
        @foreach (var i in filter)
        {
            if (i.Class == "63cntt4")   
            {
                <ul class="list-group list-group-horizontal">
                    <li class="list-group-item px-2" style="width:100px">@i.mssv</li>
                    <li class="list-group-item px-2" style="width:200px">@i.Name</li>
                    <li class="list-group-item px-2" style="width:100px">@i.Class</li>
                </ul>
            }
        }

    </div>
</body>
</html>
